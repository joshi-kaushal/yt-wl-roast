{
  "manifest_version": 3,
  "name": "YT Watch Later Roaster",
  "version": "1.0.0",
  "description": "Roasts a user based on their watch later videos.",
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "https://www.youtube.com/*"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "./icons/icon16.ico",
    "32": "./icons/icon32.ico",
    "48": "./icons/icon48.ico",
    "128": "./icons/icon128.ico"
  }
}
